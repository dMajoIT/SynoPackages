#!/bin/sh
LOG=/var/log/MODS_Sample_Advanced_CGI
case $1 in
  start)
    echo `date` "Advanced Test CGI is installed properly & started" >> $LOG
    exit 0
    ;;
  stop)
    echo `date` "Advanced Test CGI is stopped" >> $LOG
    exit 0
    ;;
  status)
    exit 0
    ;;
  log)
    echo $LOG
    exit 0
    ;;
esac